{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.4",
        "hardevine/shoppingcart": "^3.1",
        "jeroennoten/laravel-adminlte": "^3.8",
        "laravel/framework": "^8.75",
        "laravel/jetstream": "^2.6",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5",
        "laravelcollective/html": "^6.3",
        "livewire/livewire": "^2.5",
        "maatwebsite/excel": "^3.1",
        "spatie/laravel-permission": "^5.5",
        "ua-parser/uap-php": "^3.9"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.8",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "arcanedev/log-viewer": "~8.0.0",
        "nunomaduro/collision": "^5.10",
        "sentry/sentry-laravel": "^2.11",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        
        "testdox": "XDEBUG_MODE=coverage vendor/bin/phpunit --stop-on-failure --testdox --coverage-clover phpunit.coverage.xml --log-junit phpunit.report.xml",
        "test": ["vendor/bin/phpunit --verbose --testdox"],
        "test-ci": ["php -d pcov.enabled=1 -d pcov.directory=. -d pcov.exclude=\"~vendor~\" vendor/bin/phpunit -d memory_limit=512M"],
        "phpcs": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php --using-cache=no",
        "phpcs_validate": "vendor/bin/php-cs-fixer fix --dry-run --diff",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
